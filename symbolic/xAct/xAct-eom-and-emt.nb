     RowBox[{
      RowBox[{
       RowBox[{"action", "[", "freeScalar", "]"}], "=", "\n", "\t\t", 
       RowBox[{
        RowBox[{"1", "/", "2"}], " ", 
        RowBox[{"measure", "[", "metric", "]"}], " ", "kinScalar"}]}], ";", 
      "\n", "\t", 
      RowBox[{
       RowBox[{"action", "[", "conformalScalar", "]"}], "=", "\n", "\t\t", 
