Cell[TextData[{
 Cell[BoxData[
  RowBox[{"measure", "[", "metric_", "]"}]],ExpressionUUID->
  "0a80ee46-3de0-42ab-a9c4-3b22ce52ce1a"],
 " is to give the square root of the metric determinant.\n",
 Cell[BoxData[
  RowBox[{"tensorSim", "[", "expr_", "]"}]],ExpressionUUID->
  "53ac13c3-5d51-4cb3-9115-5ca867706e68"],
 " is useful for simplifying expressions."
}], "Text",ExpressionUUID->"38ab4524-4dc8-490d-ab8f-315439d1c26f"],

Cell[BoxData[{
 RowBox[{
  RowBox[{
   RowBox[{
    RowBox[{"measure", "[", 
     RowBox[{"metric_", "?", "MetricQ"}], "]"}], " ", ":=", "\n", "    ", 
    RowBox[{"Sqrt", "[", 
     RowBox[{
      RowBox[{"SignDetOfMetric", "@", "metric"}], " ", 
      RowBox[{"ToExpression", "[", 
       RowBox[{"\"\<Det\>\"", "<>", 
        RowBox[{"ToString", "@", "metric"}], "<>", "\"\<[]\>\""}], "]"}]}], 
     "]"}]}], ";"}], "\n"}], "\n", 
 RowBox[{
  RowBox[{
   RowBox[{"tensorSim", "[", "expr_", "]"}], " ", ":=", "\n", "    ", 
   RowBox[{
    RowBox[{"expr", "//", "ContractMetric"}], "//", "Simplification"}]}], 
  ";"}]}], "Code",
 CellLabel->"In[8]:=",ExpressionUUID->"71970fbf-eecf-464f-9678-33d14a400cac"]
}, Open  ]],

